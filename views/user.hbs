<table class="table table-striped table-dark" style="margin-top: 100px; width: 40%; margin-left: 30%;">
    <th scope="col">#</th>
    <th scope="col">info</th>
    {{#with json}}
    
    <tr>
        <th scope="row">id</th>
        <td> {{id}}</td>
    </tr>
    <tr>
        <th scope="row">name</th>
        <td> {{name}}</td>
    </tr>
    <tr>
        <th scope="row">email</th>
        <td> {{email}}</td>
    </tr>
    <tr>
        <th scope="row">adrees</th>
        <td> 
        <table class="table table-striped table-dark">
            <tr> 
                {{#with address}}
                <td>{{street}}</td>
                <td>{{suite}}</td>
                <td>{{city}}</td>
                <td>{{zipcode}}</td>
                {{/with}}
            </tr></table>
        
        <tr>
            <th scope="row">Phone</th>
            <td> {{phone}}</td>
        </tr>
        <tr>
            <th scope="row">Website</th>
            <td> {{website}}</td>
        </tr>
        <tr>
            <th scope="row">Company</th>
            <td> <table class="table">
            <tr> 
                {{#with company}}
                <td>{{name}}</td>
                <td>{{catchPhrase}}</td>
                <td>{{bs}}</td>
                {{/with}}
            </tr></table></td></tr>
    </td></tr>
    {{/with}}
</table>